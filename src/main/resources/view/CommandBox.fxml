<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<HBox prefHeight="25.0" prefWidth="855.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane prefHeight="25.0" prefWidth="731.0" styleClass="stack-pane">
        <TextField fx:id="commandTextField" onAction="#handleCommandEntered" onKeyPressed="#handleKeyPress" prefHeight="25.0" prefWidth="695.0" promptText="Enter command here..." />
      </StackPane>
      <DatePicker fx:id="date" onAction="#selectDate" prefHeight="25.0" prefWidth="123.0" promptText="Date" />
   </children>
</HBox>
