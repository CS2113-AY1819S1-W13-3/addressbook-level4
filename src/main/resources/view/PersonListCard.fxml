<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane prefWidth="264.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="190.33331298828125" minWidth="10.0" prefWidth="190.33331298828125" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="127.33331298828125" minWidth="10.0" prefWidth="73.66668701171875" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" />
            <RowConstraints />
            <RowConstraints />
         </rowConstraints>
         <children>
            <HBox alignment="CENTER_LEFT" spacing="5">
               <children>
                  <Label fx:id="id" styleClass="cell_big_label">
                     <minWidth>
                        <Region fx:constant="USE_PREF_SIZE" />
                     </minWidth>
                  </Label>
                  <Label fx:id="title" styleClass="cell_big_label" text="\$title" />
               </children>
            </HBox>
            <Label fx:id="moduleCodes" styleClass="cell_small_label" text="\$moduleCodes" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="deadline" styleClass="cell_small_label" text="\$deadline" GridPane.columnIndex="1" />
            <Label text="Module code:" GridPane.rowIndex="1" />
            <Label text="Description:" GridPane.rowIndex="2" />
            <Label fx:id="description" styleClass="cell_small_label" text="\$description" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label fx:id="priorityLevel" styleClass="cell_small_label" text="\$priorityLevel" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label text="Priority:" GridPane.rowIndex="3" />
            <Label fx:id="expectedNumOfHours" text="\$expectedNumOfHours" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Label text="To complete in: " GridPane.rowIndex="4" />
            <Label fx:id="status" styleClass="cell_small_label" text="\$status" GridPane.rowIndex="5" />
         </children>
      </GridPane>
      <HBox prefHeight="30.0" prefWidth="264.0" spacing="10">
         <children>
            <Label text="Milestones:" />
            <FlowPane fx:id="milestones" hgap="10" prefHeight="0.0" prefWidth="200.0" />
         </children>
      </HBox>
   </children>
</VBox>
